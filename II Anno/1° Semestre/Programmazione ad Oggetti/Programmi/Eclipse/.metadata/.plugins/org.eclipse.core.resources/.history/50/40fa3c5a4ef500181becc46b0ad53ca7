package main_package;

import java.awt.*;
import javax.swing.*;

public class Main {
	public static void main(String[] args) {
		Color magenta =new Color(1.0F,0.0F,1.0F);
		CerchioComponent cerchio1=new CerchioComponent(50,50,100,Color.BLUE);
		CerchioComponent cerchio2=new CerchioComponent(10,100,100,magenta);
		JFrame finestra=new JFrame();
		finestra.setSize(500,500);
		finestra.setTitle("Due cerchi colorati");
		finestra.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		finestra.add(cerchio1);
		finestra.add(cerchio2);
		finestra.setVisible(true);
	}

}
