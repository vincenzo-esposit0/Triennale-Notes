package Main_package;

import java.util.*;
import javax.swing.*;

public class main {

	public static void main(String[] args) {
		String input = JOptionPane.showInputDialog("Inserisci prezzo");
		int x = Integer.parseInt(input);
		input +=1;
		x+=1;
		System.out.println("Ho letto: " + x +" " +input);
		System.exit(0);
	}
}
